{
  "name": "angular-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "npm run ng build --prod --build-optimizer --vendor-chunk --common-chunk --service-worker",
    "start": "npm run ng serve --aot --proxy-config=proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run ng test",
    "test:ci": "npm run  npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "postinstall": "npm run prettier -s",
    "precommit": "pretty-quick --staged",
    "generate": "ng generate",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer ./dist/stats.json",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -t --hideGenerator"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@agm/snazzy-info-window": "^1.0.0-beta.5",
    "@angular/animations": "~7.0.0",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^7.3.3",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/pwa": "^0.10.3",
    "@angular/router": "~7.0.0",
    "@angular/service-worker": "~7.0.0",
    "@angular/typescript": "^2.0.0-5d0e199",
    "@auth0/angular-jwt": "^2.0.0",
    "@datorama/akita": "^1.12.5",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.7",
    "@fortawesome/free-brands-svg-icons": "^5.4.2",
    "@fortawesome/free-regular-svg-icons": "^5.4.2",
    "@fortawesome/free-solid-svg-icons": "^5.4.2",
    "@ng-bootstrap/ng-bootstrap": "^5.0.0",
    "@ngx-translate/core": "^11.0.0",
    "bootstrap": "^4.1.3",
    "chart.js": "^2.8.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "lodash": "^4.17.11",
    "material-design-icons-iconfont": "^4.0.2",
    "ngx-logger": "^3.3.4",
    "ngx-owl-carousel": "^2.0.7",
    "owl.carousel": "^2.3.4",
    "popper.js": "^1.15.0",
    "rxjs": "~6.3.3",
    "snazzy-info-window": "^1.1.0",
    "sweetalert": "^2.1.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/cli": "~7.0.3",
    "@angular/compiler-cli": "^7.2.9",
    "@angular/language-service": "~7.0.0",
    "@compodoc/compodoc": "^1.1.10",
    "@types/google-maps": "^3.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.1",
    "codelyzer": "~4.5.0",
    "ember-cli-auto-port": "0.0.1",
    "htmlhint": "^0.10.1",
    "https-proxy-agent": "^2.2.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.12.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "protractor": "~5.4.0",
    "standard-version": "^4.4.0",
    "stylelint": "^9.7.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.3.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "~3.1.1"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
